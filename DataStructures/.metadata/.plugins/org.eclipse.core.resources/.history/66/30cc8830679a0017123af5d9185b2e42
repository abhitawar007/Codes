package tests;
import tree.BinaryTree;
import tree.Node;

public class TreeTest 
{
	public TreeTest()
	{
		System.out.println("========== Tree Test ===========");
		
		
		//print inorder traversal of two trees
		
		BinaryTree tree1 = new BinaryTree();
		BinaryTree tree2 = new BinaryTree();
		Node t1 = tree1.getRoot();
		Node t2 = tree2.getRoot();
		tree1.insert(t1, 15);
		tree1.insert(t1, 11);
		tree1.insert(t1, 19);
		tree1.insert(t1, 17);
		tree1.insert(t1, 13);
		
		tree1.insert(t2, 14);
		tree1.insert(t2, 12);
		tree1.insert(t2, 16);
		tree1.insert(t2, 10);
		tree1.insert(t2, 18);
		
	}
}
